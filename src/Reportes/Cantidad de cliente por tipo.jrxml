	<queryString>
		<![CDATA[SELECT
    CASE
        WHEN c.tipocliente = 'EMPRESA' THEN 'EMPRESA'
        WHEN c.tipocliente = 'PERSONA' THEN 'PERSONA NATURAL'
        ELSE 'OTRO'
    END AS tipo_cliente,
    COUNT(*) AS cantidad_clientes
FROM cliente c
WHERE c.estado = True
GROUP BY
    CASE
        WHEN c.tipocliente = 'EMPRESA' THEN 'EMPRESA'
        WHEN c.tipocliente = 'PERSONA' THEN 'PERSONA NATURAL'
        ELSE 'OTRO'
    END
ORDER BY tipo_cliente;]]>
