	<queryString>
		<![CDATA[SELECT
    s.servicio AS "Nombre del Servicio",
    COUNT(dc.idservicio) AS "Cantidad de Veces Solicitado",
    SUM(dc.precioventa * dc.cantidad) AS "Ingresos Totales Generados",
    ROUND(AVG(dc.precioventa), 2) AS "Precio Promedio (Ticket)"
FROM
    servicio s
JOIN
    detalle_cita dc ON s.idservicio = dc.idservicio
JOIN
    cita c ON dc.idcita = c.idcita
GROUP BY
    s.servicio
ORDER BY
    "Ingresos Totales Generados" DESC;]]>
	</queryString>
