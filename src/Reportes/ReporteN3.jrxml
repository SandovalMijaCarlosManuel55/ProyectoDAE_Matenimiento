	<queryString>
		<![CDATA[SELECT
    mv.marcavehiculo AS marca,
    s.servicio AS falla_o_servicio,
    COUNT(dc.iddetallecita) AS frecuencia
FROM
    marca_vehiculo mv
JOIN
    modelo_vehiculo mod ON mv.idmarcavehiculo = mod.idmarcavehiculo
JOIN
    vehiculo v ON mod.idmodelovehiculo = v.idmodelovehiculo
JOIN
    cita c ON v.idvehiculo = c.idvehiculo
JOIN
    detalle_cita dc ON c.idcita = dc.idcita
JOIN
    servicio s ON dc.idservicio = s.idservicio

GROUP BY
    mv.marcavehiculo, s.servicio
ORDER BY
    mv.marcavehiculo, frecuencia DESC;]]>
	</queryString>
