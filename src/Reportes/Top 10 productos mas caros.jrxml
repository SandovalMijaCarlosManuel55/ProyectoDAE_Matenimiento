	<queryString>
		<![CDATA[select
    p.idproducto,
    p.producto,
    mp.marcaproducto,
    tp.tipoproducto,
    p.precioactual,
    p.stock,
    p.vigencia
from producto p
join marca_producto mp
    on mp.idmarcaproducto = p.idmarcaproducto
join tipo_producto tp
    on tp.idtipoproducto = p.idtipoproducto
where p.vigencia = true
order by p.precioactual desc
limit 10;]]>
	</queryString>
