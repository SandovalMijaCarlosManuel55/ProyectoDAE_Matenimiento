	<queryString>
		<![CDATA[select
    mp.marcaproducto,
    sum(p.stock * p.precioactual) as valor_inventario
from producto p
join marca_producto mp
    on mp.idmarcaproducto = p.idmarcaproducto
where p.vigencia = true
group by mp.marcaproducto
order by valor_inventario desc;]]>
	</queryString>
