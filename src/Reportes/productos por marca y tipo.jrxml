	<queryString>
		<![CDATA[select
    p.idproducto,
    p.producto,
    mp.marcaproducto,
    tp.tipoproducto,
    p.stock,
    p.precioactual,
    case 
        when p.vigencia = true then 'VIGENTE'
        else 'NO VIGENTE'
    end as estado_vigencia
from producto p
join marca_producto mp
    on mp.idmarcaproducto = p.idmarcaproducto
join tipo_producto tp
    on tp.idtipoproducto = p.idtipoproducto
order by
    tp.tipoproducto,
    mp.marcaproducto,
    p.producto;]]>
	</queryString>
