	<queryString>
		<![CDATA[SELECT
    p.idproducto,
    p.producto AS nombre,
    p.stock,
    p.precioactual,
    (p.stock * p.precioactual) AS valor_inventario,
    CASE
        WHEN p.stock <= 5 THEN 'BAJO STOCK'
        WHEN p.stock BETWEEN 6 AND 20 THEN 'STOCK NORMAL'
        ELSE 'STOCK ALTO'
    END AS estado_stock,
    p.vigencia
FROM producto p
ORDER BY p.producto;]]>
	</queryString>
